@model FrontEnd_MVC.Models.Notoriete

@{
    ViewData["Title"] = "Activate";
}

<h1>Activer une notoriété</h1>


<h3>Etes-vous sûre de vouloir activer cette notoriété ?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Idnotoriete)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Idnotoriete)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Libelle)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Libelle)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CoefficientMultiplicateur)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CoefficientMultiplicateur)
        </dd>
       
    </dl>
    
    <form asp-action="Activate">
        <input type="submit" value="Activer" class="btn btn-success" /> | <a class="btn btn-dark" asp-action="AfficheNotorieteInactive">Retour à la liste</a>
        
    </form>
</div>
